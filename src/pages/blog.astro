---
import { getCollection } from "astro:content";
const blogEntries = await getCollection("blog");
---

<ul>
  {
    blogEntries.map((blogPostEntry) => (
      <li>
        <a href={`/my-blog-url/${blogPostEntry.slug}`}>
          {blogPostEntry.data.title}
        </a>
      </li>
    ))
  }
</ul>
